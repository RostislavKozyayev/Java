<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>

<VBox alignment="CENTER" spacing="20" xmlns="http://javafx.com/javafx/17"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.app_javafx.controller.MainController">
    <padding>
        <Insets top="50" right="30" bottom="50" left="30"/>
    </padding>

    <Label fx:id="welcomeLabel" text="Добро пожаловать!"
           style="-fx-font-size: 24; -fx-font-weight: bold;"/>

    <Label text="Вы успешно вошли в систему!" style="-fx-font-size: 16;"/>

    <Button text="Выйти" onAction="#onLogoutButtonClick"
            style="-fx-font-size: 14; -fx-pref-width: 100;"/>
</VBox>